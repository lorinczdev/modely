{
    "name": "lorinczdev/modely",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Author Name",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/lorinczdev/modely",
    "keywords": [
        "Laravel",
        "Modely"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "~9",
        "guzzlehttp/guzzle": "^7.4",
        "spatie/ray": "^1.36",
        "spatie/laravel-ray": "^1.30"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "barryvdh/laravel-ide-helper": "^2.12",
        "vimeo/psalm": "^4.24",
        "psalm/plugin-laravel": "^2.0",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest-plugin-mock": "^1.0",
        "nunomaduro/collision": "^6.2",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Lorinczdev\\Modely\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lorinczdev\\Modely\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lorinczdev\\Modely\\ModelyServiceProvider"
            ],
            "aliases": {
                "Modely": "Lorinczdev\\Modely\\Facades\\Modely"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
